trans=function(max,data,newname,sp){
newname=matrix(0,ncol=sp+1,nrow=max)
newname[,1]=data[,1]
for (j in 1:max){
for (i in 1:sp){
newname[j,i+1]=sum(data[j,(32*32/sp*(i-1)+2):(32*32/sp*i+1)])
}
}
newname=as.data.frame(newname)
names(newname)=c("index1",paste("x",1:sp,sep=""))
return(newname)
}
